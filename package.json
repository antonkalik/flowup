{
  "name": "flowup",
  "version": "1.0.0",
  "description": "Programming flowchart editor with math of data.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js --open",
    "build": "npm run clean && npm run netlify-config && webpack --config ./config/webpack.prod.js",
    "clean": "rimraf dist && mkdir dist",
